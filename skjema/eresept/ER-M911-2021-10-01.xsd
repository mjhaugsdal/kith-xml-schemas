<schema
        xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.ehelse.no/xmlstds/eresept/m911/2021-10-01" elementFormDefault="qualified"
        attributeFormDefault="unqualified">
    <import namespace="http://www.kith.no/xmlstds" schemaLocation="../../felleskomponenter/kith.xsd"/>
    <element name="M911">
        <complexType>
            <sequence>
                <element name="Fnr" type="string">
                    <annotation>
                        <documentation>Fødselsnummer på pasienten det søkes etter Varer i bruk for.</documentation>
                    </annotation>
                </element>
                <element name="InklReseptinfo" type="boolean" minOccurs="0">
                    <annotation>
                        <documentation>Angivelse av om svaret skal inneholde informasjon om alle tilgjengelige resepter
                            for pasient.
                        </documentation>
                    </annotation>
                </element>
                <element name="InklM25Meldinger" type="boolean" minOccurs="0">
                    <annotation>
                        <documentation>Angivelse av om svaret skal inneholde siste versjon av M25.1, M25.2 og M25.3
                            dersom de finnes i Reseptformidleren
                        </documentation>
                    </annotation>
                </element>
                <element name="AnstattId" type="string" minOccurs="0"/>
                <element name="Komprimering" type="int">
                    <annotation>
                        <documentation>Integer pga kompatibilitet med annen bruk, men fungerer som boolean. "0 (eller
                            mindre)": svaret blir ikke komprimert. "1 (eller større)": svaret blir komprimert.
                        </documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>
</schema>
